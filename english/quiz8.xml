<?xml version="1.0"?>
<!DOCTYPE quiz [
	<!ELEMENT quiz (title, items)>
	<!ELEMENT title (#PCDATA)>
	<!ELEMENT items (item)+>	
	<!ELEMENT item (question, answer, answer+)>
	<!ELEMENT question (#PCDATA)>
	<!ELEMENT answer (#PCDATA)>
	<!ATTLIST answer correct (y) #IMPLIED>
]>
<quiz>
	<items>
		<item>
			<question>1. The extension of presentation file is ___________ </question>
			<answer>.DOC</answer>
			<answer>.XLS</answer>
			<answer>.PTP</answer>
			<answer correct="y">none of the above</answer>
		</item>
		<item>
			<question>2. Presentation is opened in  __________View.</question>
			<answer>Slide </answer>
			<answer correct="y">Normal </answer>
			<answer>Outline</answer>
			<answer>Master</answer>
		</item>		
		
		<item>
			<question>3. The first slide is the _________ Slide</question>
			<answer>Initial </answer>
			<answer>Final</answer>
			<answer correct="y">Title</answer>	
			<answer>Master</answer>
			
		</item>		
		<item>
			<question>4. You can add animation effects and change play settings by clicking________________ button on the Slide Show menu </question>
			<answer correct="y">Custom Animation</answer>
			<answer>Rehearse Timing</answer>
